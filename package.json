		"format": "prettier --write src/",
		"lint:style": "stylelint 'src/**/*.{vue,scss,html}' --fix",
		"lint:all": "pnpm lint && pnpm format && pnpm lint:style",
		"changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
